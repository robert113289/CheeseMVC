@{

    

}
<h1> My Cheese</h1>
<p><a asp-controller="Cheese" asp-action="Add">Add Cheese</a></p>
<p><a asp-controller="Cheese" asp-action="Remove">Remove Cheese</a></p>
@if (ViewBag.Cheeses.Count == 0)
{
    <p>No results to display</p>
}

<ul>
    @foreach (var KeyValuePair in ViewBag.cheeses)
    {
        <li>Cheese: @KeyValuePair.Key  Description: @KeyValuePair.Value</li>
       

    }
</ul>


    

